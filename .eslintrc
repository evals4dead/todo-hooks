{
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "semi": "error",
        "react-hooks/rules-of-hooks": "error"
    },
    "plugins": [
        "react-hooks"
    ]
}